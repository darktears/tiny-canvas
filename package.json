{
  "name": "tinycanvas",
  "version": "1.0.0",
  "description": "A little tiny canvas demos",
  "main": "src/main-application.js",
  "snowpack": {
    "webDependencies": [
      "lit-element",
      "lit-html",
      "lit-html/directives/class-map.js",
      "@material/mwc-button",
      "@material/mwc-icon-button",
      "@material/mwc-snackbar",
      "workbox-window"
    ],
    "dedupe": [
      "lit-element",
      "lit-html"
    ]
  },
  "scripts": {
    "start": "concurrently --kill-others \"babel src/ --watch --out-dir dist/\" \"es-dev-server --node-resolve --watch --babel --open index.html --port 8081\"",
    "build": "rimraf web_modules && snowpack --optimize && babel src/ --out-dir dist/ && workbox generateSW workbox-config.js",
    "regen-package-lock": "rimraf node_modules && rimraf package-lock.json; npm install"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "nan": "^2.14.1",
    "workbox-window": "^5.1.3",
    "@material/mwc-button": "^0.15.0",
    "@material/mwc-icon-button": "^0.15.0",
    "@material/mwc-snackbar": "^0.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "babel-eslint": "10.1.0",
    "concurrently": "^5.2.0",
    "es-dev-server": "^1.47.0",
    "eslint": "^6.8.0",
    "eslint-plugin-lit": "1.2.0",
    "rimraf": "^3.0.2",
    "snowpack": "^1.7.1",
    "workbox-cli": "^5.1.3"
  },
  "keywords": [],
  "license": "BSD-3-Clause",
  "author": {
    "name": "Alexis Menard",
    "email": "alexis.menard@intel.com"
  }
}
